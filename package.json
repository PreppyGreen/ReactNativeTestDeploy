{
  "name": "reactnativetestdeploy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:build": "react-native run-ios --configuration Release",
    "postios:build": "rm -rf ReactNativeTestDeploy.app && cp -R ios/build/ReactNativeTestDeploy/Build/Products/Release-iphonesimulator/ReactNativeTestDeploy.app .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:e2e": "npm run ios:build && cd e2e/iOS && mvn clean && mvn test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reset": "watchman watch-del-all && npm cache clear --force && npm i && rm -rf /tmp/metro-*"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-mapbox-gl/maps": "^7.0.9",
    "@types/axios": "^0.14.0",
    "@types/babel__core": "^7.1.3",
    "@types/eslint": "^6.1.5",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.2",
    "@types/react-native-autocomplete-input": "^4.0.1",
    "@types/react-native-elements": "^0.18.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-navigation": "^3.4.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/typescript": "^2.0.0",
    "appcenter": "^2.6.1",
    "appcenter-analytics": "^2.6.1",
    "appcenter-auth": "^2.6.1",
    "appcenter-crashes": "^2.6.1",
    "appcenter-push": "^2.6.1",
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-autocomplete-input": "^4.1.0",
    "react-native-config": "^0.12.0",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.27",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^2.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.8.0",
    "@react-native-community/eslint-config": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.56.4",
    "react-test-renderer": "16.9.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-navigation|@react-navigation/.*|@react-native-community/masked-view))"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "resolutions": {
    "react-native-gesture-handler": "^1.1.0"
  }
}
